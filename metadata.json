{
  "author": "Benjamin Merot",
  "dependencies": [
    {
      "name": "puppetlabs/stdlib",
      "version_requirement": ">= 2.2.0 <5.0.0"
    }
  ],
  "description": "Combining libvirt and Kickstart to create and setup a fresh and up to date virtual machine in just a few minutes.\nAll the configuration is defined in Puppet without the need for a dedicated server to distribute Kickstart configuration files.",
  "license": "GPL-3.0",
  "name": "tuxomatic-vkick",
  "project_page": "https://github.com/tux-o-matic/puppet-vkick",
  "issues_url": "https://github.com/tux-o-matic/puppet-vkick/issues",
  "source": "https://github.com/tux-o-matic/puppet-vkick",
  "summary": "Module to define KVM virtual machines and their OS configuration through Kickstart files.",
  "version": "0.5.0",
  "requirements": [
    {
      "name": "puppet",
      "version_requirement": ">= 3.0.0 < 5.0.0"
    }
  ],
  "operatingsystem_support": [
    {
      "operatingsystem": "RedHat",
      "operatingsystemrelease": [
        "6",
        "7"
      ]
    },
    {
      "operatingsystem": "CentOS",
      "operatingsystemrelease": [
        "6",
        "7"
      ]
    },
    {
      "operatingsystem": "OracleLinux",
      "operatingsystemrelease": [
        "6",
        "7"
      ]
    },
    {
      "operatingsystem": "Scientific",
      "operatingsystemrelease": [
        "6",
        "7"
      ]
    },
    {
      "operatingsystem": "Fedora",
      "operatingsystemrelease": [
        "21",
        "22",
        "23",
        "24",
        "25"
      ]
    }
  ]
